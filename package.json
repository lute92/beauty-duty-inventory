{
  "name": "express-api-example",
  "version": "1.0.0",
  "description": "Express API for product information",
  "main": "app/main.js",
  "scripts": {
    "start": "npm run service",
    "service": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "build": "tsc",
    "clean": "rm -Rf dist",
    "clean:dist": "rm -rf dist",
    "clean:npm": "rm -rf node_modules",
    "clean:all": "rm -rf dist node_modules"
  },
  "keywords": [
    "express",
    "api",
    "mongodb"
  ],
  "author": "Sithu Kyaw",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/mongoose": "^5.11.97",
    "@types/supertest": "^2.0.12",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "mongoose": "^7.1.1",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "mongodb": "^5.5.0"
  }
}
